    "version": "0.2.0",
    "configurations": [
        {
            "name": "CUDA C++: Launch",
            "type": "cuda-gdb",
            "request": "launch",
            "program": "${workspaceFolder}/build/htm_flow",
            "envFile": "${workspaceFolder}/.vscode/var_env.env",
            "breakOnLaunch": true,
        },
        {
            "name": "Test Overlap GPU CUDA C++",
            "type": "cuda-gdb",
            "request": "launch",
            "program": "${workspaceFolder}/build/cuda/gpu_overlap_exec",
            "envFile": "${workspaceFolder}/.vscode/var_env.env",
            "breakOnLaunch": true,
        },
        {
            "name": "Test Overlap GPU gdb",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/cuda/gpu_overlap_exec",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            // "preLaunchTask": ""
        },
        {
            "name": "(gdb) Unit Tests Overlap",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/htm_flow_tests",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            // "preLaunchTask": ""
        }
    ]
}