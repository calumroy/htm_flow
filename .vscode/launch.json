    "version": "0.2.0",
    "configurations": [
        {
            "name": "CUDA C++: htm_flow",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/htm_flow",
            "envFile": "${workspaceFolder}/.vscode/var_env.env",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            // "preLaunchTask": ""
        },
